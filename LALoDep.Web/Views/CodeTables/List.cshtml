@model LALoDep.Models.Administration.CodeTableViewModel
@{
    ViewBag.Title = "Code Type";
}

<style>
    .widget-buttons label {
        width: 100%;
        display: block;
    }

    .widget-buttons .form-control {
        height: 25px;
        width: 200px;
        display: inline;
        margin-left: 8px;
    }

    .Standard .widget-buttons label {
        padding-top: 5px;
    }
</style>
<div class="row" id="divSearchResult">
    <div class="col-md-12 col-sm-12 center-block" style="padding: 0 20px 0 20px">
        <div class="widget row">
            <div class="widget-header bordered-bottom bordered-themeprimary col-xs-12">
                <span class="widget-caption">Code Type</span>
                <div class="widget-buttons">
                    &nbsp;
                </div>
            </div>
            <div class="widget-body col-xs-12">
                <div class="table-responsive">
                    <table class="table table-bordered table-condensed table-hover table-padding table-striped" id="CodeTableView">
                        <thead>
                            <tr>
                                <th class="">Code Type</th>                              
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript">
        var $onViewLoad = '@Model.OnViewLoad';
    </script>
    <script src="@Url.Content("~/Scripts/Application-Script/Administration/CodeTable.js?v=" + Guid.NewGuid())"></script>
      
}
